{
    "compilerOptions": {
      "target": "es5",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "paths": {
        "src/*": ["src/*"],
        "utils/*": ["./src/utils/*"],
        "configs/*": ["./configs/*"],
        "containers/*": ["./src/containers/*"],
        "components/*": ["./src/components/*"],
        "hooks/*": ["./src/hooks/*"],
        "pages/*": ["./src/pages/*"],
        "store/*": ["./src/store/*"],
        "static/*": ["./src/static/*"],
        "types/*": ["./src/types/*"]
      },
      "typeRoots": [
        "src/types" // 指定 d.ts 文件的位置，根据具体情况修改
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react",
      "baseUrl": ".",
    },
    "include": [
      "src"
    ],
    "exclude": [
      "./node_modules"
    ]
  }